<?xml version='1.0' encoding='UTF-8'?>
<!--Proposed Tool Section: [Eco-Metabolomics]-->
<tool id="ecomet_06_import_traits" name="ecomet_06_import_traits" version="0.5">
  <requirements>
    <container type="docker">container-registry.phenomenal-h2020.eu/phnmnl/ecomet</container>
  </requirements>
  <description>Import ecological characteristics file from the MTBLS520 study.</description>
  <stdio>
    <regex match="" source="stderr" level="warning" description="R messages" />
    <exit_code range="1:" level="fatal" description="Tool error" />
  </stdio>
  <command><![CDATA[
mtbls520_06_import_traits.r $infile1 $infile2 $rdata;
  ]]>
  </command>
  <inputs>
    <param name="infile1" type="data" format="rdata" optional="False" label="Input Preprocess Rdata file." />
    <param name="infile2" type="data" format="csv" optional="False" label="Input CSV file." />
  </inputs>
  <outputs>
    <data name="rdata" type="data" format="rdata" label="Preprocess Rdata file" />
  </outputs>
  <help><![CDATA[
This is the MTBLS520 study.
The study will be published soon. This code is work-in-progress. Do not redistribute unless study published.
Copyright (C) 2017 Kristian Peters - IPB Halle
----
More updates soon
  ]]></help>
</tool>

